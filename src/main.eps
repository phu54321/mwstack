import intro;
import location as loc;
import lifesys;
import patternbase;
import moveloop;
import shootsys;

function onPluginStart() {
    lifesys.initLifeSys();
}


function afterTriggerExec() {
    SetInvincibility(Enable, '(any unit)', AllPlayers, 'Anywhere');
    SetResources(AllPlayers, SetTo, 5000, Ore);

    patternbase.loopPatterns();
    intro.introLoop();
    lifesys.lifeSysLoop();
    moveloop.moveLoop();
    shootsys.shootLoop();
}
