import patternbase;
import patterns.common;
import location as loc;

function impl(p: patternbase.Pattern) {
    const afX, afY = loc.getTopLeft('allyField');
    loc.pxMoveLocation('pxmove', afX + 32 * p.x, afY + 32 * p.y, 64, 64);
    MoveUnit(All, '(men)', p.player, 'Anywhere', 'pxmove');
    return 0;
}
